{
  "name": "game-pencil-engine-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prettier:ci": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prettier:diff": "prettier --list-different \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:md",
    "lint:ci": "yarn lint:js --quiet && yarn lint:style && yarn lint:md",
    "lint:js": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:md": "markdownlint **/*.md --ignore-path .eslintignore",
    "lint:style": "stylelint \"**/*.css\""
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@mdx-js/react": "^2.1.1",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.14",
    "@tsconfig/docusaurus": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.30.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended": "^6.0.0",
    "typescript": "^4.5.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
